function fucksynapse()
    local slowupdatespeeds = Drawing.new("Text")
    slowupdatespeeds.Color = Color3.fromRGB(190, 190, 0)
    slowupdatespeeds.Size = 20
    slowupdatespeeds.Outline = true
    slowupdatespeeds.Center = true
    return slowupdatespeeds
end

