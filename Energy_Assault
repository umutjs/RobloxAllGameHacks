function synapsecantsavescripts(nigger)
    if nigger.Character ~= nil and nigger.Character:FindFirstChild("HumanoidRootPart") and nigger.Character:FindFirstChild("Head") then
        if wayyyyy and nigger.Team ~= ass.Team or not wayyyyy and nigger ~= ass then
            return true
        end
    end
    return false
end

function synapsecrashessooomuch(part, pos)
    local FarPosition = its:WorldToViewportPoint(Vector3.new(part.Position.X, part.Position.Y + (part.Size.Y / 2), part.Position.Z))
    return FarPosition.Y - pos.Y
end

function synapsexsucks(nigger)
    local TopY = math.huge -- not changing these its to much work and already done
    local BottomY = -math.huge
    local RightX = -math.huge
    local LeftX = math.huge
    local Offsets
    local Positions = {}
    for i, v in next, nigger.Character:GetChildren() do
        if v.ClassName == "MeshPart" or v.Name == "Head" then
            local Position, OnScreen = its:WorldToViewportPoint(v.Position)
            Positions[v.Name] = Position
            Offsets = synapsecrashessooomuch(v, Position)
            if OnScreen then
                if Position.Y < TopY then
                    TopY = Position.Y
                end
                if Position.Y > BottomY then
                    BottomY = Position.Y
                end
                if Position.X < LeftX then
                    LeftX = Position.X
                end
                if Position.X > RightX then
                    RightX = Position.X
                end
            end
        end
    end
    return {TopLeft = Vector2.new(LeftX + Offsets, TopY + Offsets), TopRight = Vector2.new(RightX - Offsets, TopY + Offsets), BottomLeft = Vector2.new(LeftX + Offsets, BottomY - Offsets), BottomRight = Vector2.new(RightX - Offsets, BottomY - Offsets), Positions = Positions}
end

function eatmyass()
    local bro = nil
    local just = math.huge
    for i, v in next, sooooooo:GetPlayers() do 
        if v ~= ass and synapsecantsavescripts(v) then 
            local ifuckinggiveup, nou = its:WorldToScreenPoint(v.Character["HumanoidRootPart"].Position) 
            if nou then
                local assnigga = ass:GetMouse()
                local synapseisdogwater = (Vector2.new(assnigga.X, assnigga.Y) - Vector2.new(ifuckinggiveup.X, ifuckinggiveup.Y)).Magnitude
                if not synapse.gay and synapseisdogwater < just or synapse.gay and synapseisdogwater < just and synapseisdogwater < synapse.use then 
                    bro = v 
                    just = synapseisdogwater
                end
            end
        end
    end
    return bro 
end

function AddEsp(nigger)
    local jjsploitisfuckingbetter = ihatesynapse()
    local jew = fucksynapse()
    fuck.Stepped:Connect(function()
        fagexists = synapsecantsavescripts(nigger)
        if fagexists and nigger.Character:FindFirstChild("HumanoidRootPart") then
            local niggers = synapsexsucks(nigger)
            local xDist = niggers.BottomRight.X - niggers.TopLeft.X
            local yDist = niggers.BottomRight.Y - niggers.TopLeft.Y
            local i_fucking_hate_synapse_sooo_much_its_so_ass_why_cant_someone_just_make_a_good_damn_script_executer_damnit, fagsvisible = its:WorldToViewportPoint(nigger.Character.HumanoidRootPart.Position)
            if synapse.krnl then
                jjsploitisfuckingbetter.Visible = fagsvisible
                jjsploitisfuckingbetter.Size = Vector2.new(xDist, yDist)
                jjsploitisfuckingbetter.Position = niggers.TopLeft
            else
                jjsploitisfuckingbetter.Visible = false
            end
            if synapse.i then
                jew.Visible = fagsvisible
                jew.Position = Vector2.new(niggers.BottomRight.X - (xDist / 2), niggers.TopLeft.Y - 45)
                jew.Text = nigger.Name
            else
                jew.Visible = false
            end
        else
            jjsploitisfuckingbetter.Visible = false
            jew.Visible = false
        end
    end)
end

